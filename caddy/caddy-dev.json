{
  "admin": {
    "disabled": false,
    "listen": "127.0.0.1:2010"
  },
  "apps": {
    "http": {
      "servers": {
        "dev": {
          "listen": [
            ":80"
          ],
          "routes": [
            {
              "match": [
                {
                  "host": [
                    "localhost"
                  ]
                }
              ],
              "handle": [
                {
                  "handler": "file_server",
                  "root": "/var/www/dist",
                  "index_names": [
                    "index.html"
                  ]
                }
              ]
            }
          ]
        },
        "example": {
          "listen": [
            ":2015"
          ],
          "routes": [
            {
              "match": [
                {
                  "host": [
                    "localhost"
                  ]
                }
              ],
              "handle": [
                {
                  "handler": "static_response",
                  "body": "Hello, world!"
                }
              ]
            }
          ]
        }
      }
    }
  }
}
